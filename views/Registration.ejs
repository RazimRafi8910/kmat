   document.getElementById('confirm_test').addEventListener('click', function () {

    console.log("called")
    // Gather form data
    const form = document.forms["add_details"];
    const formData = {
        name: form["name"].value,
        kmat_id: form["mobile"].value,
        gender: form["email"].value,
        gender: form["gender"].value,
        dob: form["dob"].value,
        guardianName: form["guardianName"].value,
        guardianNumber: form["guardianNumber"].value,
        religion: form["religion"].value,
        caste: form["caste"].value,
        income: form["income"].value,
        houseName: form["houseName"].value,
        streetName: form["streetName"].value,
        place: form["place"].value,
        city: form["city"].value,
        pincode: form["pincode"].value,
        postOffice: form["postOffice"].value,
        district: form["district"].value,
        state: form["state"].value,
        category: form["category"].value,
        streamStudied: form["streamStudied"].value,
        collage: form["college"].value,
        schoolAddress: form["schoolAddress"].value,
        NLPtraining: form["NLP-training"].value,
    };

    // Make the POST request with fetch
    fetch("/register", {
        method: "POST",
        headers: {
            "Content-Type": "application/json"
        },
        body: JSON.stringify(formData)
    })
    .then(response => response.text()) // Assuming the server returns a plain text response
    .then(data => {
        console.log(data);
        // Display success message
        const msg = `<p style="color:orange;"><i><b>your registration is success, <br> Thank you for registering with EMET!<br></b></i></p>`;
        document.getElementById('btn1').style.display = 'block';
        document.getElementById('ssss').innerHTML = msg;
        setTimeout(() => {
            location.reload();
        }, 5000);
    })
    .catch(error => {
        console.error("Error:", error);
        // Display error message
        const msg = `<p style="color:orange;"><i><b><strong>Sorry...!!!</strong> This email is already existing. Unable to submit your form. Please try again.</b></i></p>`;
        document.getElementById('btn2').style.display = 'block';
        document.getElementById('ffff').innerHTML = msg;
        // Reload the page after 5 seconds
        setTimeout(() => {
            location.reload();
        }, 5000);
    });
});



